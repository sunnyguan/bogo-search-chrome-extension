{
  "manifest_version": 3,
  "name": "Leetcode rooms",
  "version": "1.0",
  "description": "RIP binarysearch",
  "content_scripts": [
    {
      "js": ["scripts/socket.io.2.js", "scripts/constants.js", "scripts/utils.js", "scripts/content.js"],
      "matches": [
        "https://leetcode.com/problems/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://*.leetcode.com/"
  ],
  "icons": {
    "128": "128.png",
    "64": "64.png"
  }
}